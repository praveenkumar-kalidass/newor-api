{
  "name": "newor-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./app.js --port=3000",
    "start:dev": "nodemon ./app.js --port=3000",
    "db:user": "node ./script/create-user.js",
    "db:create": "node ./script/create-database.js",
    "migration": "sequelize db:migrate",
    "migration:create": "sequelize migration:create",
    "migration:undo": "sequelize db:migrate:undo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:report": "jest --silent --coverage"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "express": "~4.16.1",
    "jest": "^27.5.1",
    "joi": "^17.6.0",
    "morgan": "~1.9.1",
    "password-hash": "^1.2.2",
    "pg": "^8.7.3",
    "sequelize": "^6.16.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1"
  }
}
